<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/profession_item"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    style="@style/search_result_item">
    <TextView
        android:id="@+id/profession_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="111"
        style="@style/search_result_item_primary"/>
    <TextView
        android:id="@+id/region_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="222"
        style="@style/search_result_item_secondary"/>
</LinearLayout>